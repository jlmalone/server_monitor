{
  "service": {
    "identifier": "com.servermonitor.test-https",
    "name": "Test HTTPS Server",
    "command": [
      "/usr/local/bin/node",
      "/path/to/project/https-server.js"
    ],
    "path": "/path/to/project",
    "enabled": true,
    "keepAlive": {
      "SuccessfulExit": false
    },
    "throttleInterval": 10,
    "environmentVariables": {
      "PATH": "/usr/local/bin:/usr/bin:/bin",
      "NODE_ENV": "development"
    },
    "port": 3000,
    "healthCheck": "https://localhost:3000"
  },
  "settings": {
    "logDir": "/tmp/servermonitor/logs",
    "identifierPrefix": "com.servermonitor"
  },
  "_comment": "Golden fixture for cross-platform plist generation validation. Swift and Node.js implementations should produce identical XML output for this input."
}
